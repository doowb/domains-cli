{
  "name": "domains-cli",
  "description": "Check to see if domain names are available using the DNSimple api.",
  "version": "0.2.0",
  "homepage": "https://github.com/doowb/domains-cli",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/domains-cli",
  "bugs": {
    "url": "https://github.com/doowb/domains-cli/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "bin": {
    "domains": "cli.js"
  },
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "array-unique": "^0.3.2",
    "async-array-reduce": "^0.2.1",
    "data-store": "^0.16.1",
    "dnsimple": "^2.4.0",
    "extend-shallow": "^2.0.1",
    "lazy-cache": "^2.0.2",
    "log-utils": "^0.2.1",
    "longest": "^1.0.1",
    "pad-right": "^0.2.2",
    "write-json": "^0.2.2",
    "yargs-parser": "^4.2.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "dnsimple",
    "domain",
    "domain-name",
    "domains",
    "hostname",
    "website"
  ],
  "verb": {
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
